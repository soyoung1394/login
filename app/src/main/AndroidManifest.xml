<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sinabro.test">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SelectiveActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".DisabledRegisterActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".DisabledMainActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".DisabledLoginActivity" />
        <activity android:name=".ServiceActivity"/>
        <activity android:name=".PrivacyActivity"/>


    </application>

</manifest>